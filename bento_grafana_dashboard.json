            "editorMode": "code",
            "format": "table",
            "rawQuery": true,
            "rawSql": "\nSELECT\n  job_id,\n  res2.start_time,\n  res1.total_cycles,\n  res2.elapsed_sec,\n  CAST(res1.total_cycles AS decimal)  / res2.elapsed_sec AS hz\nFROM\n(\n  SELECT\n    job_id,\n    output->'total_cycles' total_cycles\n  FROM tasks\n  WHERE task_id = 'init'\n) res1\nJOIN\n(\n  SELECT \n    job_id as id2,\n    MIN(started_at) as start_time,\n    EXTRACT(EPOCH FROM (MAX(updated_at) - MIN(started_at))) AS elapsed_sec\n  FROM tasks GROUP BY job_id\n) res2\nON res1.job_id = res2.id2\n",
            "refId": "A",
